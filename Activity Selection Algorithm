#include <bits/stdc++.h>
using namespace std;


void activityprob(int s[], int f[], int n)
{
	int i=0, j;
	for (j = 1; j < n; j++) {
		if (s[j] >= f[i]) {
			cout << "A" << j <<" ";
			i = j;
		}
	}
}

int main()
{
    char activity = {'a',
	int s[] = { 1, 3, 0, 5, 8, 5 };
	int f[] = { 2, 4, 6, 7, 9, 9 };
	int n = sizeof(s) / sizeof(s[0]);
	activityprob(s, f, n);
	return 0;
}

